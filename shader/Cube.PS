Texture2D texture0 : register(t0);
SamplerState sampler0 : register(s0);

struct VS_INPUT {
    float3 pos : POSITION;
	float2 texcoord : TEXCOORD;
};

struct VS_OUTPUT {
    float4 pos : SV_POSITION;
	float2 texcoord : TEXCOORD;
};

float4 PS_Main(VS_OUTPUT input) : SV_TARGET
{
    return texture0.Sample(sampler0, input.texcoord);
}